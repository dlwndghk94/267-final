#!/bin/bash -l
#SBATCH -p debug
#SBATCH -t 00:10:00
#SBATCH -A mp309
#SBATCH -o openmp24.%j.stdout
#SBATCH -e openmp24.%j.error
#SBATCH -N 1
export OMP_NUM_THREADS = 1
srun -n 1 -c 1 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 2
