export OMP_NUM_THREADS = 7
srun -n 1 -c 7 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 8
srun -n 1 -c 8 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 9
srun -n 1 -c 9 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 10
srun -n 1 -c 10 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 11
srun -n 1 -c 11 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 12
srun -n 1 -c 12 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 13
srun -n 1 -c 13 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 14
srun -n 1 -c 14 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 15
srun -n 1 -c 15 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 16
srun -n 1 -c 16 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 17
srun -n 1 -c 17 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 18
srun -n 1 -c 18 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 19
srun -n 1 -c 19 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 20
srun -n 1 -c 20 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 21
srun -n 1 -c 21 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 22
srun -n 1 -c 22 ./particle_filtler_timed.out
export OMP_NUM_THREADS = 23
